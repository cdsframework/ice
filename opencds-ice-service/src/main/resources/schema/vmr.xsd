<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:vmr="org.opencds.vmr.v1_0.schema.vmr" xmlns:dt="org.opencds.vmr.v1_0.schema.datatypes" targetNamespace="org.opencds.vmr.v1_0.schema.vmr">
	<xs:annotation>
		<xs:documentation>Specifies information about a patient relevant for CDS.  NOTE: this version of the schema deviates from the HL7 balloted VMR model in the following ways: 1.) ClinicalStatementEntityInRoleRelationship.role was renamed to ClinicalStatementEntityInRoleRelationship.targetRole; 2.) ClinicalStatementEntityInRoleRelationship.relationshipTimeInterval was added; 3.) relatedEntityInRole was renamed to relatedEntity, and RelatedClinicalStatementToEntityInRole was deleted.  These changes produce a consistent structure for deeply nested Entity elements within a ClinicalStatement.</xs:documentation>
	</xs:annotation>
	<xs:import namespace="org.opencds.vmr.v1_0.schema.datatypes" schemaLocation="datatypes.xsd"/>
	<!--== Extended VMR Datatypes ==-->
	<xs:complexType name="BodySite">
		<xs:annotation>
			<xs:documentation>A location on an EvaluatedPerson's body.  E.g., left breast, heart.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="bodySiteCode" type="dt:CD" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A location on an EvaluatedPerson's body.  May or may not encompass laterality.  E.g., lung, left lung.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="laterality" type="dt:CD" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The side of the body, from the EvaluatedPerson's perspective. E.g., left, right, bilateral.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DoseRestriction">
		<xs:annotation>
			<xs:documentation>Referred to in CDA release 2 as maxDoseQuantity.  Specifies the maximum dose that can be given in a specified time interval.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="maxDoseForInterval" type="dt:PQ">
				<xs:annotation>
					<xs:documentation>Maximum amount of substance that can be given within the specified time interval.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="timeInterval" type="dt:PQ">
				<xs:annotation>
					<xs:documentation>The time interval during which the dose specified is the maximum amount that should be administered.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--== Entities ==-->
	<xs:complexType name="EntityBase" abstract="true">
		<xs:annotation>
			<xs:documentation>Abstract base class for a physical thing, group of physical things, an organization, etc. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>TemplateId, Id, and EntityType</xs:documentation>
			</xs:annotation>
			<xs:element name="templateId" type="dt:II" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The identifier of a set of constraints placed on an Entity.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" type="dt:II">
				<xs:annotation>
					<xs:documentation>The entity's unique identifier.  Used for internal tracking purposes; must be provided.  Does not need to be the entity's "real" identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="entityType" type="dt:CD" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The specific type of entity.  E.g., healthcare organization, medical facility, pacemaker.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdministrableSubstanceSimple">
		<xs:annotation>
			<xs:documentation>A material of a particular constitution that can be given to a person to enable a clinical effect.  It can have component administrable substances.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="vmr:EntityBase">
				<xs:sequence>
					<xs:element name="substanceCode" type="dt:CD">
						<xs:annotation>
							<xs:documentation>The code that identifies the substance with as much specificity as appropriate, or as required by a template.  E.g., aspirin, lisinopril.  May be either a generic or brand code, unless otherwise restricted by a template.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="strength" type="dt:RTO" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The concentration of the substance.  E.g., 250 mg per 5 ml.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="form" type="dt:CD" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The physical form of the substance as presented to the subject. E.g., tablet, patch, injectable, inhalant.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="substanceBrandCode" type="dt:CD" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A code describing the product as a branded or trademarked entity from a controlled vocabulary.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="substanceGenericCode" type="dt:CD" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A code describing the product as a substance produced and distributed without patent protection.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="manufacturer" type="dt:CD" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The organization that produces the substance.  This is a CD and not an II because there are managed code systems for manufacturers.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lotNo" type="dt:ST" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The number assigned by the manufacturer to the batch of manufactured substances in which this substance instance belongs.  Used for quality control purposes.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AdministrableSubstance">
		<xs:annotation>
			<xs:documentation>An administrable substance which may have component substances, such as specific antigens, diluents, etc.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="vmr:AdministrableSubstanceSimple">
				<xs:sequence>
					<xs:element name="relatedEntity" type="vmr:RelatedEntity" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>An administrable substance may have component substances, such as specific antigens, diluents, etc.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EntitySimple" abstract="false">
		<xs:annotation>
			<xs:documentation>A simple physical thing </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="vmr:EntityBase">
				<xs:sequence>
					<xs:element name="description" type="dt:ST" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Description or trivial name of the entity</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Entity" abstract="false">
		<xs:annotation>
			<xs:documentation>An entity which may contain other related entities, such as a published guideline which may contain numbered rules.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="vmr:EntitySimple">
				<xs:sequence>
					<xs:element name="relatedEntity" type="vmr:RelatedEntity" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>An entity may contain other related entities, such as a published guideline which may contain numbered rules.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EvaluatedPerson">
		<xs:annotation>
			<xs:documentation>A person who is the subject of evaluation by a CDS system.  May be the focal patient or some other relevant person (e.g., a relative or a sexual contact). Includes demographic attributes, clinical statements, and related entities.
			
Entity-entity, clinical statement-clinical statement, and entity-clinical statement relationships may be represented through direct nesting of content and/or through the use of the relationship entities directly attached to the EvaluatedPerson.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="vmr:EntityBase">
				<xs:sequence>
					<xs:element name="demographics">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="birthTime" type="dt:TS" minOccurs="0">
									<xs:annotation>
										<xs:documentation>The date on which the person was born.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="age" type="dt:PQ" minOccurs="0">
									<xs:annotation>
										<xs:documentation>The person's age at the time of CDS evaluation.  May potentially be provided instead of birthTime when birthTime is not available. E.g., 3.5 months, 63 years.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="gender" type="dt:CD" minOccurs="0">
									<xs:annotation>
										<xs:documentation>The person's gender.  E.g., male, female.  Typically will consist of administrative gender, with clinical gender noted using ObservationEvents.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="race" type="dt:CD" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>The person's race.  Race is a classification of humans into large groups by various factors, such as heritable phenotypic characteristics or geographic ancestry.  E.g., White, Asian.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="ethnicity" type="dt:CD" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>The person's ethnicity.  An ethnicity or ethnic group is a group of people whose members identify with each other through a common heritage.  E.g., Hispanic.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="name" type="dt:EN" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>A word or a combination of words by which a person is known.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="address" type="dt:AD" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>The place or the name of the place where a person is located or may be reached.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="telecom" type="dt:TEL" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>A locatable resource of a person that is identified by a URI, such as a web page, a telephone number (voice, fax or some other resource mediated by telecommunication equipment), an e-mail address, or any other locatable resource that can be specified by a URL.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="isDeceased" type="dt:BL" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Whether the person is deceased.

Included to support family history-based inferencing.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="ageAtDeath" type="dt:PQ" minOccurs="0">
									<xs:annotation>
										<xs:documentation>The age at which the person died.

Included to support family history-based inferencing.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="preferredLanguage" type="dt:CD" minOccurs="0">
									<xs:annotation>
										<xs:documentation>The person's language of preference.  E.g., English.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="relatedEntity" type="vmr:RelatedEntity" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>This is any kind of thing, person, organization, that has a relationship to the EvaluatedPerson  However, this element may not be used for any person who may also have relevant medical data (they go in OtherEvaluatedPersons)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="clinicalStatements">
						<xs:annotation>
							<xs:documentation>All ClinicalStatements for one EvaluatedPerson are placed in this element.

RelatedClinicalStatements (such as Observations about the root ClinicalStatement can be nested within their root ClinicalStatements, as they are normally done in messages, or they can be simply listed in this element, and their relationship to the root ClinicalStatement placed in the relationship lists below.

Similarly, related Entities can either be nested, or placed in the EntityLists element below, </xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="adverseEvents" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="adverseEvent" type="vmr:AdverseEvent" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="deniedAdverseEvents" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="deniedAdverseEvent" type="vmr:DeniedAdverseEvent" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="encounterEvents" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="encounterEvent" type="vmr:EncounterEvent" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="missedAppointments" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="missedAppointment" type="vmr:MissedAppointment" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="scheduledAppointments" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="scheduledAppointment" type="vmr:ScheduledAppointment" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="appointmentRequests" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="appointmentRequest" type="vmr:AppointmentRequest" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="appointmentProposals" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="appointmentProposal" type="vmr:AppointmentProposal" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="goals" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="goal" type="vmr:Goal" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="goalProposals" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="goalProposal" type="vmr:GoalProposal" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="observationResults" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="observationResult" type="vmr:ObservationResult" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="unconductedObservations" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="unconductedObservation" type="vmr:UnconductedObservation" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="observationOrders" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="observationOrder" type="vmr:ObservationOrder" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="observationProposals" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="observationProposal" type="vmr:ObservationProposal" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="problems" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="problem" type="vmr:Problem" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="deniedProblems" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="deniedProblem" type="vmr:DeniedProblem" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="procedureEvents" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="procedureEvent" type="vmr:ProcedureEvent" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="undeliveredProcedures" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="undeliveredProcedure" type="vmr:UndeliveredProcedure" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="scheduledProcedures" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="scheduledProcedure" type="vmr:ScheduledProcedure" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="procedureOrders" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="procedureOrder" type="vmr:ProcedureOrder" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="procedureProposals" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="procedureProposal" type="vmr:ProcedureProposal" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="substanceAdministrationEvents" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="substanceAdministrationEvent" type="vmr:SubstanceAdministrationEvent" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="undeliveredSubstanceAdministrations" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="undeliveredSubstanceAdministration" type="vmr:UndeliveredSubstanceAdministration" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="substanceAdministrationOrders" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="substanceAdministrationOrder" type="vmr:SubstanceAdministrationOrder" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="substanceAdministrationProposals" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="substanceAdministrationProposal" type="vmr:SubstanceAdministrationProposal" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="substanceDispensationEvents" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="substanceDispensationEvent" type="vmr:SubstanceDispensationEvent" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="supplyEvents" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="supplyEvent" type="vmr:SupplyEvent" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="undeliveredSupplies" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="undeliveredSupply" type="vmr:UndeliveredSupply" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="supplyOrders" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="supplyOrder" type="vmr:SupplyOrder" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="supplyProposals" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="supplyProposal" type="vmr:SupplyProposal" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="clinicalStatementRelationships" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Optional, not used if all related ClinicalStatements are nested within the ClinicalStatements above.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="clinicalStatementRelationship" type="vmr:ClinicalStatementRelationship" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="clinicalStatementEntityInRoleRelationships" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Optional, not used if all Entities that play a role in a ClinicalStatement are nested within the ClinicalStatements above.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="clinicalStatementEntityInRoleRelationship" type="vmr:EntityRelationship" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="entityRelationships" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Optional, not used if all Entities related to this EvaluatedPerson are nested within RelatedEntity and ClinicalStatement lists above</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="entityRelationship" type="vmr:EntityRelationship" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="entityLists" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Optional, not used if Entities related to the EvaluatedPerson are nestedd within their RelatedEntities and ClinicalStatements above..</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="administrableSubstances" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="administrableSubstance" type="vmr:AdministrableSubstance" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="entities" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="entity" type="vmr:Entity" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="facilities" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="facility" type="vmr:Facility" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="organizations" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="organization" type="vmr:Organization" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="persons" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="person" type="vmr:Person" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="specimens" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="specimen" type="vmr:Specimen" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FacilitySimple">
		<xs:annotation>
			<xs:documentation>A property such as a building that has been established to enable the performance of specific activities, typically by organizations.  E.g., a hospital or clinic.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="vmr:EntityBase">
				<xs:sequence>
					<xs:element name="name" type="dt:EN" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>A word or a combination of words by which a facility is known.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="address" type="dt:AD" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The place or the name of the place where a facility is located or may be reached.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="telecom" type="dt:TEL" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>A locatable resource of a facility that is identified by a URI, such as a web page, a telephone number (voice, fax or some other resource mediated by telecommunication equipment), an e-mail address, or any other locatable resource that can be specified by a URL.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Facility">
