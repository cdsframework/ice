						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="person">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="vmr:Person"/>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="specimen">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="vmr:Specimen"/>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:sequence>
